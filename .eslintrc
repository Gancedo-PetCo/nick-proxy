{
  "extends": "airbnb",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "no-undef": 0,
    "prefer-arrow-callback": 0,
    "no-unused-expressions": 0,
    "spaced-comment": 0,
    "space-before-function-paren": 0,
    "no-plusplus": 0,
    "max-len": 0,
    "no-else-return": 0,
    "quote-props": 0,
    "func-names": 0,
    "arrow-body-style": 0,
    "react/prop-types": 0,
    "class-methods-use-this": 0,
    "no-mixed-operators": 0,
    "dot-notation": 0,
    "prefer-object-spread": 0,
    "import/no-extraneous-dependencies": 0, 
    "no-restricted-syntax": 0,
    "guard-for-in": 0,
    "consistent-return": 0,
    "no-shadow": 0,
    "global-require": 0,
    "import/no-dynamic-require" : 0,
  }
}